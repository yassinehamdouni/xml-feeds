{
  "builds": [{ "src": "*.py", "use": "@vercel/python"}],
  "routes": [
    {
      "src": "/workable/(?<id>[^/]+)",
      "dest": "/src/workable.py?id=$id"
    },
    {
      "src": "/workable_master/(?<id>[^/]+)",
      "dest": "/src/master_xml_workable.py?id=$id" 
    }
    { "src": "/(.*)", "dest": "/" }
  ],
  "github": {
    "silent": true
  }
}
